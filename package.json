{
  "name": "dashp",
  "version": "0.9.0",
  "description": "Utilities for monadic promises.",
  "main": "_dist/index.js",
  "module": "lib/index.js",
  "config": {
    "srcDir": "lib",
    "testDir": "test",
    "distDir": "_dist",
    "benchmarks": "./benchmarks/**/*.suite.js"
  },
  "scripts": {
    "commitmsg": "! git rev-parse --abbrev-ref HEAD | grep -q master || validate-commit-msg",
    "precommit": "! git rev-parse --abbrev-ref HEAD | grep -q master || (lint-staged && npm run test)",
    "prepublishOnly": "npm run build",
    "publish": "git push --follow-tags origin master",
    "test": "cross-env NODE_ENV=test mocha -c -R progress -r babel-register -r test/helpers.js $npm_package_config_testDir/**/*.spec.js",
    "coverage": "nyc npm test",
    "lint": "eslint $npm_package_config_testDir $npm_package_config_srcDir $npm_package_config_benchmarks",
    "fix": "npm run lint -- --fix",
    "compile": "npm run clean -s && babel -d $npm_package_config_distDir $npm_package_config_srcDir --source-maps",
    "clean": "rimraf $npm_package_config_distDir",
    "build": "npm run lint -s && npm run test -s && npm run compile -s",
    "release": "npm run build -s && standard-version -i Changelog.md -m \"chore(release): Bumped to version %s.\" --sign",
    "release-candidate": "npm run release -- --prerelease \"rc\""
  },
  "repository": "https://github.com/critocrito/dashp.git",
  "keywords": [
    "async",
    "promise",
    "promises",
    "promises-aplus",
    "functional",
    "static-land",
    "monad",
    "monads",
    "monadic"
  ],
  "author": "Christo <christo@cryptodrunks.net>",
  "license": "GPL-3.0",
  "bugs": "https://github.com/critocrito/dashp/issues",
  "homepage": "https://github.com/critocrito/dashp#Readme.md",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "husky": "^0.14.3",
    "in-range": "^1.0.0",
    "jsverify": "github:critocrito/jsverify#throws-regex",
    "lint-staged": "^6.1.1",
    "lodash": "^4.17.5",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "standard-version": "^4.3.0",
    "time-span": "^2.0.0",
    "validate-commit-msg": "^2.14.0"
  }
}
