{
  "name": "dashp",
  "description": "Utilities for monadic promises.",
  "license": "GPL-3.0",
  "author": "Christo <christo@cryptodrunks.net>",
  "homepage": "https://github.com/critocrito/dashp#readme",
  "repository": "https://github.com/critocrito/dashp.git",
  "version": "0.10.1",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "benchmarks": "for i in benchmarks/*.suite.js; do babel-node $i; done",
    "benchmarks:all": "babel-node benchmarks/all.suite.js",
    "benchmarks:curry": "babel-node benchmarks/curry.suite.js",
    "build": "yarn lint && yarn test && yarn compile",
    "clean": "rimraf dist",
    "precompile": "yarn clean",
    "compile": "babel -s -D -d dist src",
    "compile:watch": "yarn compile -w",
    "fix": "yarn lint --fix",
    "lint": "eslint test src benchmarks",
    "prepublishOnly": "yarn build",
    "prerelease": "yarn build",
    "release": "standard-version -i Changelog.md -m \"chore(release): bumped to version %s\" --sign",
    "test": "cross-env NODE_ENV=test mocha -c -R progress -r @babel/register -r test/helpers.js test/**/*.spec.js",
    "test:coverage": "cross-env NODE_ENV=test nyc yarn test",
    "test:watch": "yarn test -w"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@build-chores/babel-preset": "0.9.0",
    "@build-chores/commitlint": "0.9.0",
    "@build-chores/eslint-config-lint": "0.9.0",
    "@build-chores/staged": "0.9.0",
    "@build-chores/test": "0.9.0",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "husky": "^1.1.1",
    "in-range": "^1.0.0",
    "jsverify": "github:critocrito/jsverify#throws-regex",
    "lodash": "^4.17.5",
    "mocha": "^6.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.7",
    "standard-version": "^5.0.0",
    "time-span": "^2.0.0"
  },
  "keywords": [
    "async",
    "functional",
    "monad",
    "monadic",
    "monads",
    "promise",
    "promises",
    "promises-aplus",
    "static-land"
  ]
}
